{
    "name": "gupalo/config-bundle",
    "description": "Config Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gupalo\\ConfigBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\ConfigBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "^2.7|^3.0",
        "gupalo/browser-notifier": "^1.0",
        "symfony/form": "^5.0",
        "symfony/notifier": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.1",
        "roave/security-advisories": "dev-master",
        "symfony/security-guard": "^5.0",
        "doctrine/doctrine-bundle": "^2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}
